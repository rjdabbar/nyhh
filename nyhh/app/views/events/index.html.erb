<% @event.each do |e| %>
<div class="row">
	<div class="col-md-3 col-md-offset-2">
		<%= image_tag e.avatar.url(:medium) %>
	</div>
	<div class="col-md-5">
		<h1><%= link_to e.event_name, e %></h1>
		<h2><%= e.description %></h2>
		<hr>
		<h2><%= e.location %></h2>
	</div>
<div class="row">
	<div class="col-md-8 col-md-offset-2">
		<div class="col-md-4 col-md-offset-2">
			<% t = Chronic.parse(e.start) %>
			<h2><%= e.ticket_price %> | <%= t.strftime("%H:%M")%> </h2>
		</div>
		<div class="col-md-4 col-md-offset-2">
			<h2><%= link_to "More Details", event_path(e.id) %></h2>
		</div>
	</div>

	</div>
	<% end %>

