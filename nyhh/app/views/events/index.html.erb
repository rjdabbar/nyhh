<% @event.each do |e| %>
<div class="row">
	<div class="col-md-3 col-md-offset-2 clip-circle">
		<%= link_to (image_tag e.avatar.url(:medium)), event_path(e.id) %>
	</div>
	<div class="col-md-5">
		<h2><%= link_to e.event_name, event_path(e.id) %></h2>
		<hr>
		<h3><%= e.venue %></h3>
		<hr>
		<% t = Chronic.parse(e.start) %>
		<h4><%= e.ticket_price %> / <%= t.strftime("%H:%M")%> &nbsp; &nbsp; &nbsp; <%= link_to "More Details", event_path(e.id) %></h4>
	</div>
<div class="row">
	<div class="col-md-8 col-md-offset-2">
		<div class="col-md-4 col-md-offset-2">
			
		</div>
		<div class="col-md-4 col-md-offset-2">
			
		</div>
	</div>

	</div>
	<% end %>

